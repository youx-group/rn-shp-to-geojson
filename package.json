{
  "name": "rn-shp-to-geojson",
  "version": "0.1.0",
  "description": "A React Native library to convert Shapefile to GeoJSON",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youx-group/rn-shp-to-geojson.git"
  },
  "author": {
    "name": "Julio Diniz",
    "username": "juceza"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youx-group/rn-shp-to-geojson/issues"
  },
  "homepage": "https://github.com/youx-group/rn-shp-to-geojson#readme",
  "keywords": [
    "react-native",
    "shapefile",
    "geojson"
  ],
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build --watch",
    "build:clean": "rimraf lib",
    "test": "",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build:clean && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "react-native-blob-util": "0.17.3",
    "react-native-fs": "2.20.0"
  },
  "devDependencies": {
    "@types/geojson": "7946.0.8",
    "@types/jest": "^29.5.0",
    "@types/node": "16.11.10",
    "jest": "^29.5.0",
    "mkdirp": "^3.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.8.3"
  }
}